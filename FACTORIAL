//Factorial
import java.io.*;
import java.util.Scanner;
class f
{
public static void main(String[]args)
{
int a,f=1;
System.out.println("Enter a number:");
Scanner s=new Scanner(System.in);
a=s.nextInt();
int temp=a;
while(a!=0)
{
f=f*a;
a--;
}
System.out.println("Factorial of " +temp+ "=" +f);
}
}
