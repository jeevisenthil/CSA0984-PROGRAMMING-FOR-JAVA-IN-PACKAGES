//Factors
import java.io.*;
import java.util.Scanner;
class s
{
public static void main(String[]args)
{
int a,c=0,i;
Scanner s=new Scanner(System.in);
System.out.println("Enter a Number:");
a=s.nextInt();
for(i=1;i<=a;i++)
{
if(a%i==0)
{
c++;
}
}
System.out.println("Number of factorsof "+a+" ="+c);
}
}
